<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_home_bottom_menu"
    android:layout_width="match_parent"
    android:layout_height="@dimen/BottomMenuCellHeight"
    android:layout_alignParentBottom="true"
    android:layout_alignParentStart="true"
    android:background="@color/colorWhite"
    android:gravity="center_horizontal"
    android:orientation="horizontal"
    android:weightSum="5">

    <RelativeLayout
        android:id="@+id/rl_bottom_menu_first_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@null"
        android:paddingBottom="@dimen/BottomMenuCellBottomPadding"
        android:paddingLeft="@dimen/BottomMenuCellLeftPadding"
        android:paddingRight="@dimen/BottomMenuCellRightPadding"
        android:paddingTop="@dimen/BottomMenuCellTopPadding">

        <ImageView
            android:id="@+id/iv_bottom_menu_first"
            android:layout_width="@dimen/BottomMenuImageWidth"
            android:layout_height="@dimen/BottomMenuImageHeight"
            android:layout_centerHorizontal="true"
            app:srcCompat="@mipmap/ic_launcher" />

        <votive.com.appuaet10.CustomUI.CustomRegularTextView
            android:id="@+id/tv_bottom_menu_first"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_bottom_menu_first"
            android:layout_centerHorizontal="true"
            android:maxLines="1"
            android:text="@string/DemoText"
            android:textColor="@color/colorBlack"
            android:textSize="11sp" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_bottom_menu_second_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@null"
        android:paddingBottom="@dimen/BottomMenuCellBottomPadding"
        android:paddingLeft="@dimen/BottomMenuCellLeftPadding"
        android:paddingRight="@dimen/BottomMenuCellRightPadding"
        android:paddingTop="@dimen/BottomMenuCellTopPadding">

        <ImageView
            android:id="@+id/iv_bottom_menu_second"
            android:layout_width="@dimen/BottomMenuImageWidth"
            android:layout_height="@dimen/BottomMenuImageHeight"
            android:layout_centerHorizontal="true"
            app:srcCompat="@mipmap/ic_launcher" />

        <votive.com.appuaet10.CustomUI.CustomRegularTextView
            android:id="@+id/tv_bottom_menu_second"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_bottom_menu_second"
            android:layout_centerHorizontal="true"
            android:maxLines="1"
            android:text="@string/DemoText"
            android:textColor="@color/colorBlack"
            android:textSize="11sp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_bottom_menu_third_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@null"
        android:paddingBottom="@dimen/BottomMenuCellBottomPadding"
        android:paddingLeft="@dimen/BottomMenuCellLeftPadding"
        android:paddingRight="@dimen/BottomMenuCellRightPadding"
        android:paddingTop="@dimen/BottomMenuCellTopPadding">


        <ImageView
            android:id="@+id/iv_bottom_menu_third"
            android:layout_width="@dimen/BottomMenuImageWidth"
            android:layout_height="@dimen/BottomMenuImageHeight"
            android:layout_centerHorizontal="true"
            app:srcCompat="@mipmap/ic_launcher" />

        <votive.com.appuaet10.CustomUI.CustomRegularTextView
            android:id="@+id/tv_bottom_menu_third"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_bottom_menu_third"
            android:layout_centerHorizontal="true"
            android:maxLines="1"
            android:text="@string/DemoText"
            android:textColor="@color/colorBlack"
            android:textSize="11sp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_bottom_menu_fourth_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@null"
        android:paddingBottom="@dimen/BottomMenuCellBottomPadding"
        android:paddingLeft="@dimen/BottomMenuCellLeftPadding"
        android:paddingRight="@dimen/BottomMenuCellRightPadding"
        android:paddingTop="@dimen/BottomMenuCellTopPadding">


        <ImageView
            android:id="@+id/iv_bottom_menu_fourth"
            android:layout_width="@dimen/BottomMenuImageWidth"
            android:layout_height="@dimen/BottomMenuImageHeight"
            android:layout_centerHorizontal="true"
            app:srcCompat="@mipmap/ic_launcher" />

        <votive.com.appuaet10.CustomUI.CustomRegularTextView
            android:id="@+id/tv_bottom_menu_fourth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_bottom_menu_fourth"
            android:layout_centerHorizontal="true"
            android:maxLines="1"
            android:text="@string/DemoText"
            android:textColor="@color/colorBlack"
            android:textSize="11sp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_bottom_menu_fifth_item"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@null"
        android:paddingBottom="@dimen/BottomMenuCellBottomPadding"
        android:paddingLeft="@dimen/BottomMenuCellLeftPadding"
        android:paddingRight="@dimen/BottomMenuCellRightPadding"
        android:paddingTop="@dimen/BottomMenuCellTopPadding">


        <ImageView
            android:id="@+id/iv_bottom_menu_five"
            android:layout_width="@dimen/BottomMenuImageWidth"
            android:layout_height="@dimen/BottomMenuImageHeight"
            android:layout_centerHorizontal="true"
            app:srcCompat="@mipmap/ic_launcher" />

        <votive.com.appuaet10.CustomUI.CustomRegularTextView
            android:id="@+id/tv_bottom_menu_five"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_bottom_menu_five"
            android:layout_centerHorizontal="true"
            android:maxLines="1"
            android:text="@string/DemoText"
            android:textColor="@color/colorBlack"
            android:textSize="11sp" />

    </RelativeLayout>
</LinearLayout>
